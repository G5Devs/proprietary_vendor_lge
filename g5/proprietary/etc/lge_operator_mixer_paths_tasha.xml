<?xml version="1.0" encoding="ISO-8859-1"?>
<mixer>
    <mpath name="headset-camcorder-mic">
        <ctl name="ADC2 Volume" value="7" />
    </mpath>

    <mpath name="voice-headphones">
        <ctl name="COMP1 Switch" value="1" />
        <ctl name="COMP2 Switch" value="1" />
        <ctl name="RX1 Mix Digital Volume" value="80" />
        <ctl name="RX2 Mix Digital Volume" value="80" />
        <ctl name="HPHL Volume" value="17" />
        <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voice-headphones-advanced">
          <ctl name="COMP1 Switch" value="1" />
          <ctl name="COMP2 Switch" value="1" />
    	  <ctl name="RX1 Mix Digital Volume" value="80" />
    	  <ctl name="RX2 Mix Digital Volume" value="80" />
    	  <ctl name="HPHL Volume" value="17" />
    	  <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voice-headphones-aux">
          <ctl name="COMP1 Switch" value="1" />
          <ctl name="COMP2 Switch" value="1" />
    	  <ctl name="RX1 Mix Digital Volume" value="80" />
    	  <ctl name="RX2 Mix Digital Volume" value="80" />
    	  <ctl name="HPHL Volume" value="17" />
    	  <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voip-headphones">
        <ctl name="COMP1 Switch" value="0" />
        <ctl name="COMP2 Switch" value="0" />
        <ctl name="RX1 Mix Digital Volume" value="84" />
        <ctl name="RX2 Mix Digital Volume" value="84" />
        <ctl name="HPHL Volume" value="17" />
        <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voip-headphones-advanced">
        <ctl name="COMP1 Switch" value="0" />
        <ctl name="COMP2 Switch" value="0" />
        <ctl name="RX1 Mix Digital Volume" value="84" />
        <ctl name="RX2 Mix Digital Volume" value="84" />
        <ctl name="HPHL Volume" value="17" />
        <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voip-headphones-aux">
        <ctl name="COMP1 Switch" value="0" />
        <ctl name="COMP2 Switch" value="0" />
        <ctl name="RX1 Mix Digital Volume" value="84" />
        <ctl name="RX2 Mix Digital Volume" value="84" />
        <ctl name="HPHL Volume" value="17" />
        <ctl name="HPHR Volume" value="17" />
    </mpath>

    <mpath name="voip-headset-mic">
        <ctl name="ADC2 Volume" value="12" />
    </mpath>

    <mpath name="mictest-handset-mic">
        <ctl name="DEC7 Volume" value="98" />
    </mpath>

    <mpath name="loopback-headphones">
        <ctl name="HPHL Volume" value="15" />
        <ctl name="HPHR Volume" value="15" />
    </mpath>
</mixer>